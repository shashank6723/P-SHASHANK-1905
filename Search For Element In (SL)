class Node:
  def __init__(self,data):
    self.data=data
    self.ref=None

class LinkedList:
  def __init__(self):
    self.head=None

  def print_LL(self):
    if self.head is None:
      print("Linked List is empty")

    else:
      n=self.head
      while n is not None:
        print(n.data,"--->",end="")
        n=n.ref

  def add_begin(self,data):
    new_node = Node(data)
    new_node.ref=self.head
    self.head=new_node

  def search(self, x):
    current = self.head
    while current != None:
      if current.data == x:
        return True
      
      current = current.ref
       
    return False

LL1= LinkedList()
LL1.add_begin(10)
LL1.add_begin(20)
LL1.add_begin(30)
LL1.add_begin(15)
LL1.add_begin(40)
LL1.print_LL()
print()
element=int(input('Enter number to search '))
if LL1.search(element):
        print("Number present in list")
else:
  print("Number not present in the list")
