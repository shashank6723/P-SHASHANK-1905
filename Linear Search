list=[]
n=int(input('Enter number of elements '))
for i in range(0,n):
  element=int(input('Enter element '))
  list.append(element)
print(list)
pivot=int(input('Enter pivot number '))
index=list.index(pivot)
if pivot in list:
  print('Number matched at position',index)
  index=list.index(pivot)
else:
  print('Number not matched')
